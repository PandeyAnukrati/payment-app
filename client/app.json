      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#667eea"
      }
    },
    "web": {},
    "plugins": [
      "expo-secure-store",
      "expo-font"
    ]
  }
}
