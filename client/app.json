{
  "expo": {
    "name": "Payment Dashboard",
    "slug": "payment-dashboard",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "splash": {
      "backgroundColor": "#667eea"
